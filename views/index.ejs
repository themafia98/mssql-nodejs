<%- include("./chunks/header"); -%>

<div class="root">
    <% if (error) {%>
        <p class = "error"><%=error %></p>
    <%} %>
    <p class="title proc">Результат выполнения процедуры</p>
    <div style = "max-width: 1140px; margin: 0 auto;">
    <div class="procedureResult">
            NO DATA
    </div>
</div>
<div id="chartsContainer"></div>
<section class="tables">
    <% if (Array.isArray(sensors) && sensors.length) {%>
        <p class="title">dbo.Sensors</p>
    <div class="sensors table">
        <% for (let i = 0; i < sensors.length; i++){ %>
            <div class="ind_item">
                <p>numS: <%=sensors[i].numS %></p>
                <p>numRegion: <%= sensors[i].numRegion %></p>
                <p>name: <%= typeof sensors[i].name === 'string' ? sensors[i].name.trim() : "" %></p>
                <p>longitude: <%= sensors[i].longitude %></p>
                <p>latitude: <%= sensors[i].latitude %></p>
            </div>
        <%} %>
    </div>
    <%} %>
    <% if (Array.isArray(indications) && indications.length) {%>
        <p class="title">dbo.Indications</p>
    <div class="indications table">
        <% for (let i = 0; i < indications.length; i++){ %>
            <div class="ind_item">
                <p>numInd: <%=indications[i].numInd %></p>
                <p>numS: <%= indications[i].numS %></p>
                <p>dateInd: <%=indications[i].dateInd %></p>
                <p>timeInd: <%=indications[i].timeInd %></p>
                <p>temp: <%=indications[i].temp %></p>
            </div>
        <%} %>
    </div>
    <%} %>
    <% if (Array.isArray(regions) && regions.length) {%>
        <p class="title">dbo.Regions</p>
    <div class="regions table">
        <% for (let i = 0; i < regions.length; i++){ %>
            <div class="ind_item">
                <p>numRegion: <%=regions[i].numRegion %></p>
                <p>nameRegion: <%= regions[i].nameRegion %></p>
            </div>
        <%} %>
    </div>
    <%} %>
</section>
</div>

<%- include("./chunks/footer"); -%>